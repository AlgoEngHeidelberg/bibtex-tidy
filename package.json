{
	"name": "bibtex-tidy",
	"version": "1.6.1",
	"description": "Tidy bibtex files",
	"homepage": "https://github.com/FlamingTempura/bibtex-tidy",
	"main": "./bibtex-tidy.js",
	"bin": {
		"bibtex-tidy": "./bin/bibtex-tidy"
	},
	"devDependencies": {
		"@babel/core": "^7.13.10",
		"@babel/preset-env": "^7.13.10",
		"@babel/preset-typescript": "^7.13.0",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^17.1.0",
		"@rollup/plugin-node-resolve": "^11.2.0",
		"@types/codemirror": "0.0.108",
		"@types/node": "^14.14.35",
		"@types/tap": "^14.10.3",
		"codemirror": "^5.60.0",
		"pegjs": "^0.10.0",
		"puppeteer": "^8.0.0",
		"rollup": "^2.42.1",
		"tap": "^14.11.0",
		"tslib": "^2.1.0",
		"typescript": "^4.1.2"
	},
	"scripts": {
		"test": "rollup -c && tsc -p test/tsconfig.json && tap --reporter classic --bail --no-coverage --test-regex=.test/test/.*\\.spec\\.js",
		"cover": "npm test --coverage-report=lcov",
		"build": "rollup -c",
		"start": "rollup -cw",
		"typecheck": "tsc --noEmit && (cd test && tsc --noEmit)"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/FlamingTempura/bibtex-tidy.git"
	},
	"author": "FlamingTempura",
	"license": "MIT"
}
